{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/ai-forecast",
      "destination": "/api/ai-forecast.js"
    },
    {
      "source": "/api/gee-data",
      "destination": "/api/gee-data.js"
    },
    {
      "source": "/api/agri-analysis",
      "destination": "/api/agri-analysis.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
