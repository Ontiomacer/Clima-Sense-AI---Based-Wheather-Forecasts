{
  "dashboard": {
    "title": "ClimaSense GraphCast Monitoring",
    "tags": ["graphcast", "weather", "ai"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(graphcast_requests_total[5m])",
            "legendFormat": "Requests/sec"
          }
        ],
        "yaxes": [
          {
            "label": "Requests/sec",
            "format": "short"
          }
        ]
      },
      {
        "id": 2,
        "title": "Cache Hit Rate",
        "type": "stat",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 6,
          "h": 8
        },
        "targets": [
          {
            "expr": "(graphcast_cache_hits / graphcast_requests_total) * 100",
            "legendFormat": "Cache Hit %"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 50,
                "color": "yellow"
              },
              {
                "value": 80,
                "color": "green"
              }
            ]
          }
        }
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "stat",
        "gridPos": {
          "x": 18,
          "y": 0,
          "w": 6,
          "h": 8
        },
        "targets": [
          {
            "expr": "(graphcast_errors_total / graphcast_requests_total) * 100",
            "legendFormat": "Error %"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 5,
                "color": "yellow"
              },
              {
                "value": 10,
                "color": "red"
              }
            ]
          }
        }
      },
      {
        "id": 4,
        "title": "Inference Time (p50, p95, p99)",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(graphcast_inference_duration_seconds_bucket[5m]))",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(graphcast_inference_duration_seconds_bucket[5m]))",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(graphcast_inference_duration_seconds_bucket[5m]))",
            "legendFormat": "p99"
          }
        ],
        "yaxes": [
          {
            "label": "Duration (seconds)",
            "format": "s"
          }
        ]
      },
      {
        "id": 5,
        "title": "API Response Time",
        "type": "graph",
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{endpoint=\"/api/graphcast_forecast\"}[5m]))",
            "legendFormat": "p50 - GraphCast"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{endpoint=\"/api/graphcast_forecast\"}[5m]))",
            "legendFormat": "p95 - GraphCast"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{endpoint=\"/api/agri_analysis\"}[5m]))",
            "legendFormat": "p50 - AgriBERT"
          }
        ],
        "yaxes": [
          {
            "label": "Duration (seconds)",
            "format": "s"
          }
        ]
      },
      {
        "id": 6,
        "title": "CPU Usage",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 16,
          "w": 8,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total[5m]) * 100",
            "legendFormat": "CPU %"
          }
        ],
        "yaxes": [
          {
            "label": "CPU %",
            "format": "percent",
            "max": 100
          }
        ]
      },
      {
        "id": 7,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {
          "x": 8,
          "y": 16,
          "w": 8,
          "h": 8
        },
        "targets": [
          {
            "expr": "process_resident_memory_bytes / 1024 / 1024 / 1024",
            "legendFormat": "Memory (GB)"
          }
        ],
        "yaxes": [
          {
            "label": "Memory (GB)",
            "format": "short"
          }
        ]
      },
      {
        "id": 8,
        "title": "GPU Utilization",
        "type": "graph",
        "gridPos": {
          "x": 16,
          "y": 16,
          "w": 8,
          "h": 8
        },
        "targets": [
          {
            "expr": "nvidia_gpu_utilization_percent",
            "legendFormat": "GPU {{gpu}}"
          }
        ],
        "yaxes": [
          {
            "label": "GPU %",
            "format": "percent",
            "max": 100
          }
        ]
      },
      {
        "id": 9,
        "title": "Cache Size",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 24,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "graphcast_cache_size_bytes / 1024 / 1024",
            "legendFormat": "Cache Size (MB)"
          },
          {
            "expr": "graphcast_cache_entries_total",
            "legendFormat": "Cache Entries"
          }
        ],
        "yaxes": [
          {
            "label": "Size / Count",
            "format": "short"
          }
        ]
      },
      {
        "id": 10,
        "title": "ERA5 Data Fetch Success Rate",
        "type": "stat",
        "gridPos": {
          "x": 12,
          "y": 24,
          "w": 6,
          "h": 8
        },
        "targets": [
          {
            "expr": "(graphcast_era5_fetch_success_total / graphcast_era5_fetch_attempts_total) * 100",
            "legendFormat": "Success %"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 90,
                "color": "yellow"
              },
              {
                "value": 95,
                "color": "green"
              }
            ]
          }
        }
      },
      {
        "id": 11,
        "title": "Request Distribution by Endpoint",
        "type": "piechart",
        "gridPos": {
          "x": 18,
          "y": 24,
          "w": 6,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum by (endpoint) (graphcast_requests_total)",
            "legendFormat": "{{endpoint}}"
          }
        ]
      }
    ],
    "templating": {
      "list": []
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "-- Grafana --",
          "enable": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "tags": ["deployment"]
        }
      ]
    }
  }
}
